function New-GadLogoPng([string]$Path,[int]$S){
  $bmp = New-Object System.Drawing.Bitmap($S,$S)
  $g = [System.Drawing.Graphics]::FromImage($bmp)
  $g.SmoothingMode = 'AntiAlias'
  $rect = New-Object System.Drawing.Rectangle(0,0,$S,$S)
  $lg = New-Object System.Drawing.Drawing2D.LinearGradientBrush($rect,
    [System.Drawing.Color]::FromArgb(0xFF,0xDC,0x7A),
    [System.Drawing.Color]::FromArgb(0xB8,0x8A,0x2B),45)
  $g.FillEllipse($lg,0,0,$S,$S)
  $g.DrawEllipse((New-Object System.Drawing.Pen ([System.Drawing.Color]::FromArgb(25,0,0,0),2)),1,1,$S-2,$S-2)
  $font = New-Object System.Drawing.Font('Arial',[single]([Math]::Round($S*0.5)),[System.Drawing.FontStyle]::Bold,[System.Drawing.GraphicsUnit]::Pixel)
  $fmt = New-Object System.Drawing.StringFormat; $fmt.Alignment='Center'; $fmt.LineAlignment='Center'
  $g.DrawString('G',$font,(New-Object System.Drawing.SolidBrush([System.Drawing.Color]::FromArgb(25,0,0,0))),($S/2)+1,($S/2)+1,$fmt)
  $g.DrawString('G',$font,(New-Object System.Drawing.SolidBrush([System.Drawing.Color]::FromArgb(0x0b,0x0f,0x17))),($S/2),($S/2),$fmt)
  $bmp.Save($Path,[System.Drawing.Imaging.ImageFormat]::Png)
  $g.Dispose(); $bmp.Dispose()
}

function New-GadHero([string]$Path,[int]$W=1600,[int]$H=900){
  $bmp = New-Object System.Drawing.Bitmap($W,$H)
  $g = [System.Drawing.Graphics]::FromImage($bmp)
  $g.SmoothingMode='AntiAlias'
  $rect = New-Object System.Drawing.Rectangle(0,0,$W,$H)
  $bg = New-Object System.Drawing.Drawing2D.LinearGradientBrush($rect,
    [System.Drawing.Color]::FromArgb(0x0b,0x0f,0x17),
    [System.Drawing.Color]::FromArgb(0x1e,0x27,0x46),90)
  $g.FillRectangle($bg,$rect)
  $ellipse = New-Object System.Drawing.Drawing2D.GraphicsPath
  $ellipse.AddEllipse(($W*0.15),($H*0.1),($W*0.7),($H*0.8))
  $rad = New-Object System.Drawing.Drawing2D.PathGradientBrush($ellipse)
  $rad.CenterColor=[System.Drawing.Color]::FromArgb(70,255,255,255)
  $rad.SurroundColors=,([System.Drawing.Color]::FromArgb(0,255,255,255))
  $g.FillPath($rad,$ellipse)
  $font = New-Object System.Drawing.Font('Arial',[single]($H/6),[System.Drawing.FontStyle]::Bold,[System.Drawing.GraphicsUnit]::Pixel)
  $fmt = New-Object System.Drawing.StringFormat; $fmt.Alignment='Center'; $fmt.LineAlignment='Center'
  $g.DrawString('GAD FAMILY',$font,(New-Object System.Drawing.SolidBrush([System.Drawing.Color]::FromArgb(20,255,255,255))),($W/2),($H/2),$fmt)
  $bmp.Save($Path,[System.Drawing.Imaging.ImageFormat]::Png)
  $g.Dispose(); $bmp.Dispose()
}

