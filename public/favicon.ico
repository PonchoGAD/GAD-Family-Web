function New-Hero {
  param([string]$Path, [int]$W=1600, [int]$H=900)
  $bmp = New-Object System.Drawing.Bitmap($W, $H)
  $g = [System.Drawing.Graphics]::FromImage($bmp)
  $g.SmoothingMode = 'AntiAlias'
  $rect = New-Object System.Drawing.Rectangle(0,0,$W,$H)

  # –ì—Ä–∞–¥–∏–µ–Ω—Ç–Ω—ã–π —Ñ–æ–Ω
  $brush = New-Object System.Drawing.Drawing2D.LinearGradientBrush($rect,
    [System.Drawing.Color]::FromArgb(0x0b,0x0f,0x17),  # —Ç—ë–º–Ω—ã–π
    [System.Drawing.Color]::FromArgb(0x1e,0x27,0x46),  # —Å–∏–Ω–µ–≤–∞—Ç—ã–π
    90)
  $g.FillRectangle($brush, $rect)

  # –°–≤–µ—Ç–ª–æ–µ ¬´–ø—è—Ç–Ω–æ¬ª –ø–æ —Ü–µ–Ω—Ç—Ä—É
  $rad = New-Object System.Drawing.Drawing2D.GraphicsPath
  $rad.AddEllipse( ($W*0.15), ($H*0.15), ($W*0.7), ($H*0.7) )
  $p = New-Object System.Drawing.Drawing2D.PathGradientBrush($rad)
  $p.CenterColor = [System.Drawing.Color]::FromArgb(60,255,255,255)
  $p.SurroundColors = ,([System.Drawing.Color]::FromArgb(0,255,255,255))
  $g.FillPath($p, $rad)

  # –¢–µ–∫—Å—Ç GAD FAMILY —Ñ–æ–Ω–æ–º
  $font = New-Object System.Drawing.Font('Arial', [single]($H/6), [System.Drawing.FontStyle]::Bold, [System.Drawing.GraphicsUnit]::Pixel)
  $fmt = New-Object System.Drawing.StringFormat
  $fmt.Alignment = 'Center'
  $fmt.LineAlignment = 'Center'
  $txtBrush = New-Object System.Drawing.SolidBrush ([System.Drawing.Color]::FromArgb(20,255,255,255))
  $g.DrawString('GAD FAMILY', $font, $txtBrush, ($W/2), ($H/2), $fmt)

  $bmp.Save($Path, [System.Drawing.Imaging.ImageFormat]::Png)
  $g.Dispose(); $bmp.Dispose()
}

function New-Logo {
  param([string]$Path, [int]$S=64)
  $bmp = New-Object System.Drawing.Bitmap($S, $S)
  $g = [System.Drawing.Graphics]::FromImage($bmp)
  $g.SmoothingMode = 'AntiAlias'
  $rect = New-Object System.Drawing.Rectangle(0,0,$S,$S)

  # –ö—Ä—É–≥ –∑–æ–ª–æ—Ç–æ–π
  $gold = [System.Drawing.Color]::FromArgb(0xFF,0xD1,0x66)
  $dark = [System.Drawing.Color]::FromArgb(0x0b,0x0f,0x17)
  $lg = New-Object System.Drawing.Drawing2D.LinearGradientBrush($rect, $gold, [System.Drawing.Color]::FromArgb(0xB8,0x8A,0x2B), 45)
  $g.FillEllipse($lg, 0,0,$S,$S)
  # –í–Ω—É—Ç—Ä–µ–Ω–Ω–µ–µ –∑–∞—Ç–µ–º–Ω–µ–Ω–∏–µ
  $g.DrawEllipse((New-Object System.Drawing.Pen ($dark,2)),1,1,$S-2,$S-2)

  # –ë—É–∫–≤–∞ G
  $fontSize = [Math]::Round($S*0.5)
  $font = New-Object System.Drawing.Font('Arial', [single]$fontSize, [System.Drawing.FontStyle]::Bold, [System.Drawing.GraphicsUnit]::Pixel)
  $fmt = New-Object System.Drawing.StringFormat
  $fmt.Alignment = 'Center'
  $fmt.LineAlignment = 'Center'
  $txtBrush = New-Object System.Drawing.SolidBrush ($dark)
  $g.DrawString('G', $font, $txtBrush, ($S/2), ($S/2), $fmt)

  $bmp.Save($Path, [System.Drawing.Imaging.ImageFormat]::Png)
  $g.Dispose(); $bmp.Dispose()
}

function New-FaviconIco {
  param([string]$PngPath, [string]$IcoPath)
  $src = [System.Drawing.Image]::FromFile($PngPath)
  $sizes = 16,32,48,64
  $imgs = @()
  foreach ($s in $sizes) { $imgs += $src.GetThumbnailImage($s,$s,$null,[intptr]::Zero) }
  $imgs[0].Save($IcoPath, [System.Drawing.Imaging.ImageFormat]::Icon)
  foreach ($i in $imgs) { $i.Dispose() }
  $src.Dispose()
}

